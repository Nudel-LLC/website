    "deploy": "opennextjs-cloudflare build && wrangler deploy"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "@trpc/client": "^11.10.0",
    "@trpc/react-query": "^11.10.0",
    "@trpc/server": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.574.0",
    "motion": "^12.34.3",
    "next": "16.1.6",
    "pino": "^10.3.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "resend": "^6.9.2",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "overrides": {
    "fast-xml-parser": "5.3.6",
